programa {
  funcao inicio() {
  inteiro dia
  escreva("informe o numero do dia da semana: ")
  leia(dia)

  se(dia==1){
    escreva("domingo")
  }senao se(dia==2){
    escreva("segunda")
  }senao se(dia==3){
    escreva("terça-feira")
  }senao se(dia==4){
    escreva("quarta-feira")
  }senao se(dia==5){
    escreva("quinta-feira")
  }senao se(dia==6){
    escreva("sexta-feira")
  }senao{
       escreva("sabado")
  }
  }
}
